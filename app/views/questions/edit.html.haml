- content_for(:page_title, "Editing: #{@question.name}")

.grid-x
  %h1 Editing Question

  = form_with model: [@grant, @question], local: true do |f|
    = render 'form', f: f, question: @question, constraint_questions: @constraint_questions

.grid-x
  = link_to 'Back to Grant', grant_path(@grant)
  \|
  = link_to 'Edit Grant', edit_grant_path(@grant)
