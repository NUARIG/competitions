<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="grid-x">
      <div class="cell">
        <%= f.text_field :uid, autocomplete: "email", placeholder: 'Email address' %>
      </div>
    </div>
    <div class="grid-x ">
      <div class="cell">
        <%= f.password_field :password, autocomplete: "current-password", placeholder: 'Password' %>
      </div>
    </div>
    <div class="grid-x">
      <div class="cell">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
    </div>
    <div class="grid-x">
      <div class="cell actions">
        <%= f.submit "Log in", class: 'button' %>
      </div>
    </div>
<% end %>
