development: # The following should be defined for every environment.
  default_url_options:
    host: # e.g. localhost
    port: # e.g. 3000
  subdomain: # e.g. ''
  app_domain: # e.g. 'localhost'
  database: # https://guides.rubyonrails.org/configuring.html#configuring-a-database
    adapter: # e.g. postgres
    host: # e.g. localhost
    database: # <DATABASE_NAME>
    username: # <DATABASE_USERNAME>
  aws: # https://edgeguides.rubyonrails.org/active_storage_overview.html#amazon-s3-service
    root: # Use for local storage. e.g. <%= Rails.root.join("storage") %>
    region: # e.g. us-east-2
    bucket: # <BUCKET_NAME>
  mailer: # https://guides.rubyonrails.org/action_mailer_basics.html#action-mailer-configuration
    mailer_address: # <APP_EMAIL_ADDRESS>
    raise_delivery_errors: # Set this to true and configure the email server for immediate delivery to raise delivery errors.
    delivery_method: # e.g. smtp
    perform_deliveries: # Determines whether deliveries are actually carried out when the deliver method is invoked on the Mail message.
  saml: # See https://github.com/apokalipto/devise_saml_authenticatable for more information
    saml_default_user_key: # The user will be looked up by this key
    sign_out_url: # URL in you SP that the IDP should send users back to
    assertion_consumer_service_url: # e.g. 'http://localhost:3000/users/saml/auth'
    assertion_consumer_service_binding: # e.g. 'urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST'
    name_identifier_format: # e.g. "urn:oasis:names:tc:SAML:2.0:nameid-format:<IDP_DEFAULT_USER_KEY>"
    issuer: # e.g. 'http://localhost:3000/users/saml/metadata'
    authn_context: # e.g. ''
    idp_slo_target_url: #IDP logout URL
    idp_sso_target_url: #IDP login URL
test:
  default_url_options:
    host:
    port:
  subdomain:
  app_domain:
  database:
    adapter:
    host:
  aws:
    region:
    bucket:
  mailer:
    mailer_address:
    raise_delivery_errors:
    delivery_method:
    perform_deliveries:
  saml:
    saml_default_user_key:
    sign_out_url:
    assertion_consumer_service_url:
    assertion_consumer_service_binding:
    name_identifier_format:
    issuer:
    authn_context:
    idp_slo_target_url:
    idp_sso_target_url:
